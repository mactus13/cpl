# Simple expressions, no precedence or associativity involved

class Main inherits IO
    o String s: [out s+"\n"]; end;
	main:
			[o 1 + 2];
			[o 123 - 1000];
			[o 3 / 4];
			[o 10 * 20];
			[o - 100];
			[o 5 < 3];
			[o 4 <= 7];
			[o 100 == 100];
			[o ! 10];
			[o null == null];
			[o null == self];
			[o 0];
	end;
end;
